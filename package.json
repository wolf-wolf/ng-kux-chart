{
  "name": "ng4-chart",
  "version": "1.0.2",
  "description": "highchart for ng4",
  "main": "index.js",
  "files": [
    "dist",
    "index.ts",
    "tsconfig.json"
  ],
  "scripts": {
    "rimraf": "rimraf",
    "start": "npm run server:dev -- --inline --hot",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base src/ --open",
    "clean:dll": "npm run rimraf -- dll",
    "clean:aot": "npm run rimraf -- compiled",
    "clean:dist": "npm run rimraf -- dist",
    "clean:demo": "npm run rimraf -- demo",
    "clean:install": "npm set progress=false && npm install",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage demo compiled dll",
    "build:aot:prod": "npm run clean:demo && npm run clean:aot && webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build": "npm run build:aot:prod",
    "webpack": "webpack",
    "preview": "http-server demo -p 3000 -o --gzip",
    "prepublish": "npm run clean:dist && gulp prepublish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wolf-wolf/ng-kux-chart.git"
  },
  "author": "hunter.li",
  "license": "ISC",
  "keywords": [
    "angular4+",
    "Component"
  ],
  "bugs": {
    "url": "https://github.com/wolf-wolf/ng-kux-chart/issues"
  },
  "homepage": "https://github.com/wolf-wolf/ng-kux-chart#readme",
